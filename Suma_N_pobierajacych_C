#include <stdio.h>
#include <stdlib.h>

int main()
{
    int N,i,x;
    int suma=0;
    printf("Podaj ilosc liczb, ktore chcesz sumowac:");
    scanf("%d",&N);
int tab[N];
for (i=0;i<N;i++)
{
    printf("podaj liczbe:");
    scanf("%d",&x);
    suma=suma+x;
}
printf("Suma wynosi:%d",suma);

    return 0;
}
